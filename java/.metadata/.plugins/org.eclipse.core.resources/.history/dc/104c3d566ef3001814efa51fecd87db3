package com.codingdojo.master;

public class Human {
	int strength =3;
	int stealth =3;
	int intelligence =3;
	int health =100;
	
	public void attack(Human attackedHuman) {
		System.out.println("Human attacked by another human and reduce the helath ");
		attackedHuman.health = attackedHuman.health - strength;
		System.out.println(attackedHuman.health);
	}

}

